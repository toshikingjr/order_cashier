      <div>
        <% age = Age.data.detect{|o| o[:id] == key} %>
        <%= "#{age[:name]}×#{value}組" %>
      </div>
    <% end %>
  </div>
  <div>
    <h3>先月の年齢層(女性)</h3>
    <% @count_women_prev_month.each do |key, value| %>
      <div>
        <% age = Age.data.detect{|o| o[:id] == key} %>
        <%= "#{age[:name]}×#{value}組" %>
      </div>
    <% end %>
  </div>
  <div>
    <h3>前年の年齢層(男性)</h3>
    <% @count_men_prev_year.each do |key, value| %>
      <div>
        <% age = Age.data.detect{|o| o[:id] == key} %>
        <%= "#{age[:name]}×#{value}組" %>
      </div>
    <% end %>
  </div>
  <div>
    <h3>前年の年齢層(女性)</h3>
